{
    "manifest_version": 3,
    "name": "Saleh Chrome Extension",
    "version": "0.1.0",
    "description": "A Chrome extension for Saleh application",
    "action": {},
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": ["<all_urls>"],
    "web_accessible_resources": [
        {
        "resources": ["popup/embed-popup.js"],
        "matches": ["<all_urls>"]
        }
    ]
}